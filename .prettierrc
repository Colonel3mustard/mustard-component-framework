{
  "trailingComma": "es5",
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "bracketSameLine": true,
  "printWidth": 100,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": ["**/*.jsonc", ".eslintrc", ".prettierrc"],
      "options": {
        "parser": "json"
      }
    }
  ]
}
